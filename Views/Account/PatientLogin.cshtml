@model AppointAid.ViewModels.LoginViewModel

@{
    ViewData["Title"] = "Patient Login";
}

<h2>Login as Patient</h2>

<form method="post" asp-action="PatientLogin">
    <input type="text" name="NationalNumber" placeholder="National Number" required />
    <input type="password" name="Password" placeholder="Password" required />
    <input type="submit" value="PatientLogin" />
</form>